    "@remix-run/react": "^2.1.0",
    "csv-parser": "^3.0.0",
    "date-fns": "^2.30.0",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-aria-components": "^1.0.0-rc.0",
    "react-dom": "^18.2.0",
    "react-stately": "^3.30.1",
    "remix-auth": "^3.6.0",
    "remix-auth-oauth2": "^1.11.1",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
